{
  "name": "win",
  "description": "Window-of-reinfection (Win) hypothesis assumes that following recovery, long-term protective immunity can take some time before becoming effective [9], result- ing in a time window of susceptibility to reinfection by the same strain.",
  "keywords": [
    "2-strains",
    "epidemiology",
    "Tristan-da-Cunha",
    "Win"
  ],
  "licenses": [
    {
      "type": "cc0",
      "url": "http://creativecommons.org/publicdomain/zero/1.0/"
    }
  ],
  "version": "0.0.0",
  "model": {
    "populations": [
      {
        "name": "TdC",
        "composition": [
          "S",
          "E",
          "I",
          "R",
          "W",
          "L"
        ]
      }
    ],
    "reactions": [
      {
        "from": "S",
        "to": "E",
        "rate": "beta*I/N",
        "description": "infection"
      },
      {
        "from": "E",
        "to": "I",
        "rate": "eps",
        "description": "infectivity",
        "tracked": [
          "Inc"
        ]
      },
      {
        "from": "I",
        "to": "R",
        "rate": "mu",
        "description": "removing"
      },
      {
        "from": "R",
        "to": "W",
        "rate": "gamma",
        "description": "reinfection window"
      },
      {
        "from": "W",
        "to": "E",
        "rate": "beta*I/N",
        "description": "reinfection"
      },
      {
        "from": "W",
        "to": "L",
        "rate": "tau",
        "description": "Long-term protective immunity"
      }
    ],
    "observations": [
      {
        "name": "cases",
        "start": "2007-08-13",
        "distribution": "discretized_normal",
        "mean": "rep * Inc",
        "sd": "sqrt(rep * ( 1.0 - rep ) * Inc )"
      }
    ],
    "data": [
      {
        "name": "cases",
        "data": [
          {
            "resource": "data",
            "field": "date"
          },
          {
            "resource": "data",
            "field": "cases"
          }
        ]
      }
    ],
    "inputs": [
      {
        "name": "beta",
        "description": "transmission rate",
        "data": {
          "resource": "r0"
        },
        "transformation": "r0/mu_inv"
      },
      {
        "name": "N",
        "description": "pop size",
        "data": {
          "resource": "N"
        }
      },
      {
        "name": "eps",
        "description": "rate leaving E",
        "data": {
          "resource": "eps_inv"
        },
        "transformation": "1/eps_inv"
      },
      {
        "name": "mu",
        "description": "recovery rate",
        "data": {
          "resource": "mu_inv"
        },
        "transformation": "1/mu_inv"
      },
      {
        "name": "gamma",
        "description": "rate leaving R",
        "data": {
          "resource": "gamma_inv"
        },
        "transformation": "1/gamma_inv"
      },
      {
        "name": "tau",
        "description": "rate leaving W",
        "data": {
          "resource": "tau_inv"
        },
        "transformation": "1/tau_inv"
      },
      {
        "name": "rep",
        "description": "reporting rate",
        "data": {
          "resource": "rep"
        }
      },
      {
        "name": "S",
        "description": "initial size of S",
        "data": {
          "resource": "S"
        }
      },
      {
        "name": "E",
        "description": "initial size of E",
        "data": {
          "resource": "E"
        }
      },
      {
        "name": "I",
        "description": "initial size of I",
        "data": {
          "resource": "I"
        }
      },
      {
        "name": "R",
        "description": "initial size of R",
        "data": {
          "resource": "R"
        }
      },
      {
        "name": "W",
        "description": "initial size of W",
        "data": {
          "resource": "W"
        }
      },
      {
        "name": "L",
        "description": "initial size of L",
        "transformation": "N-E-S-I-R-W"
      }
    ]
  },
  "resources": [
    {
      "name": "data",
      "path": "data/data.csv",
      "format": "csv",
      "schema": {
        "fields": [
          {
            "name": "date",
            "type": "date"
          },
          {
            "name": "cases",
            "type": "number"
          }
        ]
      }
    },
    {
      "name": "S",
      "description": "initial size of S",
      "data": {
        "distribution": "fixed",
        "value": 277
      }
    },
    {
      "name": "E",
      "description": "initial size of E",
      "data": {
        "distribution": "fixed",
        "value": 1
      }
    },
    {
      "name": "I",
      "description": "initial size of I",
      "data": {
        "distribution": "fixed",
        "value": 0
      }
    },
    {
      "name": "R",
      "description": "initial size of R",
      "data": {
        "distribution": "fixed",
        "value": 0
      }
    },
    {
      "name": "W",
      "description": "initial size of W",
      "data": {
        "distribution": "fixed",
        "value": 0
      }
    },
    {
      "name": "N",
      "description": "pop size",
      "data": {
        "distribution": "fixed",
        "value": 288
      }
    },
    {
      "name": "r0",
      "description": "basic reproduction number",
      "data": {
        "distribution": "uniform",
        "lower": 1,
        "upper": 50
      }
    },
    {
      "name": "eps_inv",
      "description": "duration of infection",
      "data": {
        "distribution": "uniform",
        "lower": 0.1,
        "upper": 7,
        "unit": "days"
      }
    },
    {
      "name": "mu_inv",
      "description": "recovery rate",
      "data": {
        "distribution": "uniform",
        "lower": 0.1,
        "upper": 11,
        "unit": "days"
      }
    },
    {
      "name": "gamma_inv",
      "description": "rate leaving R",
      "data": {
        "distribution": "uniform",
        "lower": 0.1,
        "upper": 30,
        "unit": "days"
      }
    },
    {
      "name": "tau_inv",
      "description": "rate leaving W",
      "data": {
        "distribution": "uniform",
        "lower": 0.1,
        "upper": 20,
        "unit": "days"
      }
    },
    {
      "name": "rep",
      "description": "reporting rate",
      "data": {
        "distribution": "uniform",
        "lower": 0,
        "upper": 1
      }
    },
    {
      "name": "values",
      "format": "json",
      "data": {
        "r0": 10.5,
        "eps_inv": 2.16,
        "mu_inv": 2.01,
        "gamma_inv": 13.6,
        "tau_inv": 4.8,
        "rep": 0.7
      }
    },
    {
      "name": "covariance",
      "format": "json",
      "data": {
        "r0": {
          "r0": 0.04
        },
        "eps_inv": {
          "eps_inv": 0.04
        },
        "mu_inv": {
          "mu_inv": 0.04
        },
        "gamma_inv": {
          "gamma_inv": 0.04
        },
        "tau_inv": {
          "tau_inv": 0.04
        },
        "rep": {
          "rep": 0.02
        }
      }
    }
  ]
}