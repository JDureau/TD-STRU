          {
            "name": "mintemp",
            "type": "number"
          },
          {
            "name": "rain",
            "type": "number"
          },
          {
            "name": "humidity",
            "type": "number"
          }
        ]
      }
    },
    {
      "name": "pr_S",
      "description": "initial size of S",
      "data": {
        "distribution": "uniform",
        "lower": 0,
        "upper": 0.2
      }
    },
    {
