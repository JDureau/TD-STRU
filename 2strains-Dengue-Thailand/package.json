{
  "name": "dengue-2strains",
  "description": "2 strains Dengue model introduced in Aguiar, M., Ballesteros, S., Kooi, B. W., & Stollenwerk, N. (2011). The role of seasonality and import in a minimalistic multi-strain dengue model capturing differences between primary and secondary infections: complex dynamics and its implications for data analysis.",
  "keywords": [
    "2-strains",
    "epidemiology",
    "Thailand"
  ],
  "licenses": [
    {
      "type": "cc0",
      "url": "http://creativecommons.org/publicdomain/zero/1.0/"
    }
  ],
  "version": "0.0.0",
  "model": {
    "populations": [
      {
        "name": "Chaiyaphum",
        "composition": [
          "S",
          "I1",
          "I2",
          "R1",
          "R2",
          "S1",
          "S2",
          "I12",
          "I21",
          "R"
        ],
        "remainder": {
          "name": "R",
          "pop_size": "N"
        }
      }
    ],
    "reactions": [
      {
        "from": "U",
        "to": "S",
        "rate": "mu_b*N",
        "description": "birth"
      },
      {
        "from": "S",
        "to": "I1",
        "rate": "beta*(1+e*mintemp/29)*(I1/N+i+psi*I21/N)",
        "description": "infection with strain 1"
      },
      {
        "from": "S",
        "to": "I2",
        "rate": "beta*(1+e*mintemp/29)*(I2/N+i+psi*I12/N)",
        "description": "infection with strain 2"
      },
      {
        "from": "I1",
        "to": "R1",
        "rate": "gamma",
        "description": "recovery from strain 1"
      },
      {
        "from": "I2",
        "to": "R2",
        "rate": "gamma",
        "description": "recovery from strain 2"
      },
      {
        "from": "R1",
        "to": "S1",
        "rate": "alpha",
        "description": "loss of cross-immunity"
      },
      {
        "from": "R2",
        "to": "S2",
        "rate": "alpha",
        "description": "loss of cross-immunity"
      },
      {
        "from": "S1",
        "to": "I12",
        "rate": "beta*(1+e*mintemp/29)*(I2/N+i+psi*I12/N)",
        "description": "reinfection",
        "tracked": [
          "Inc"
        ]
      },
      {
        "from": "S2",
        "to": "I21",
        "rate": "beta*(1+e*mintemp/29)*(I1/N+i+psi*I21/N)",
        "description": "reinfection",
        "tracked": [
          "Inc"
        ]
      },
      {
        "from": "I12",
        "to": "R",
        "rate": "gamma",
        "description": "recovery"
      },
      {
        "from": "I21",
        "to": "R",
        "rate": "gamma",
        "description": "recovery"
      },
      {
        "from": "S",
        "to": "U",
        "rate": "mu_d",
        "description": "death"
      },
      {
        "from": "I1",
        "to": "U",
        "rate": "mu_d",
        "description": "death"
      },
      {
        "from": "I2",
        "to": "U",
        "rate": "mu_d",
        "description": "death"
      },
      {
        "from": "R1",
        "to": "U",
        "rate": "mu_d",
        "description": "death"
      },
      {
        "from": "R2",
        "to": "U",
        "rate": "mu_d",
        "description": "death"
      },
      {
        "from": "S1",
        "to": "U",
        "rate": "mu_d",
        "description": "death"
      },
      {
        "from": "S2",
        "to": "U",
        "rate": "mu_d",
        "description": "death"
      },
      {
        "from": "I12",
        "to": "U",
        "rate": "mu_d",
        "description": "death"
      },
      {
        "from": "I21",
        "to": "U",
        "rate": "mu_d",
        "description": "death"
      }
    ],
    "observations": [
      {
        "name": "dataEpi",
        "start": "1981-12-01",
        "distribution": "discretized_normal",
        "mean": "rep * Inc",
        "sd": "sqrt(rep * ( 1.0 - rep ) * Inc + pow(rep*phi*Inc,2) )"
      }
    ],
    "data": [
      {
        "name": "dataEpi",
        "data": [
          {
            "resource": "dataEpi",
            "field": "date"
          },
          {
            "resource": "dataEpi",
            "field": "Chaiyaphum"
          }
        ]
      }
    ],
    "inputs": [
      {
        "name": "N",
        "data": [
          {
            "resource": "dataPop",
            "field": "date"
          },
          {
            "resource": "dataPop",
            "field": "Chaiyaphum"
          }
        ]
      },
      {
        "name": "maxtemp",
        "data": [
          {
            "resource": "dataClim",
            "field": "date"
          },
          {
            "resource": "dataClim",
            "field": "maxtemp"
          }
        ]
      },
      {
        "name": "mintemp",
        "data": [
          {
            "resource": "dataClim",
            "field": "date"
          },
          {
            "resource": "dataClim",
            "field": "mintemp"
          }
        ]
      },
      {
        "name": "rain",
        "data": [
          {
            "resource": "dataClim",
            "field": "date"
          },
          {
            "resource": "dataClim",
            "field": "rain"
          }
        ]
      },
      {
        "name": "humidity",
        "data": [
          {
            "resource": "dataClim",
            "field": "date"
          },
          {
            "resource": "dataClim",
            "field": "humidity"
          }
        ]
      },
      {
        "name": "beta",
        "description": "transmission rate",
        "data": {
          "resource": "beta"
        }
      },
      {
        "name": "psi",
        "description": "infectivity factor for people infected twice",
        "data": {
          "resource": "psi"
        }
      },
      {
        "name": "i",
        "description": "immigration",
        "data": {
          "resource": "i"
        }
      },
      {
        "name": "gamma",
        "description": "recovery rate",
        "data": {
          "resource": "gamma_inv"
        },
        "transformation": "1/gamma_inv",
        "to-resource": "1/gamma"
      },
      {
        "name": "alpha",
        "description": "loss of immunity rate",
        "data": {
          "resource": "alpha_inv"
        },
        "transformation": "1/alpha_inv",
        "to-resource": "1/alpha"
      },
      {
        "name": "e",
        "description": "amplitude of seasonal forcing",
        "data": {
          "resource": "e"
        }
      },
      {
        "name": "d",
        "description": "phase of seasonal forcing",
        "data": {
          "resource": "d"
        }
      },
      {
        "name": "S",
        "description": "initial size of S",
        "data": {
          "resource": "pr_S"
        },
        "transformation": "pr_S*N",
        "to-resource": "S/N"
      },
      {
        "name": "I1",
        "description": "initial size of I1",
        "data": {
          "resource": "pr_I1"
        },
        "transformation": "pr_I1*N",
        "to-resource": "I1/N"
      },
      {
        "name": "I2",
        "description": "initial size of I2",
        "data": {
          "resource": "pr_I2"
        },
        "transformation": "pr_I2*N",
        "to-resource": "I2/N"
      },
      {
        "name": "R1",
        "description": "initial size of R1",
        "data": {
          "resource": "pr_R1"
        },
        "transformation": "pr_R1*N",
        "to-resource": "R1/N"
      },
      {
        "name": "R2",
        "description": "initial size of R2",
        "data": {
          "resource": "pr_R2"
        },
        "transformation": "pr_R2*N",
        "to-resource": "R2/N"
      },
      {
        "name": "S1",
        "description": "initial size of S1",
        "data": {
          "resource": "pr_S1"
        },
        "transformation": "pr_S1*N",
        "to-resource": "S1/N"
      },
      {
        "name": "S2",
        "description": "initial size of S2",
        "data": {
          "resource": "pr_S2"
        },
        "transformation": "pr_S2*N",
        "to-resource": "S2/N"
      },
      {
        "name": "I12",
        "description": "initial size of I12",
        "data": {
          "resource": "pr_I12"
        },
        "transformation": "pr_I12*N",
        "to-resource": "I12/N"
      },
      {
        "name": "I21",
        "description": "initial size of I21",
        "data": {
          "resource": "pr_I21"
        },
        "transformation": "pr_S*N",
        "to-resource": "I21/N"
      },
      {
        "name": "R",
        "description": "initial size of R",
        "transformation": "N*(1-pr_S-pr_I1-pr_I2-pr_R1-pr_R2-pr_S1-pr_S2-pr_I12-pr_I21)"
      },
      {
        "name": "mu_b",
        "description": "birth rate",
        "data": {
          "resource": "mu_b"
        }
      },
      {
        "name": "mu_d",
        "description": "death rate",
        "data": {
          "resource": "mu_d"
        }
      },
      {
        "name": "phi",
        "description": "over-dispersion",
        "data": {
          "resource": "phi"
        }
      },
      {
        "name": "rep",
        "description": "over-dispersion",
        "data": {
          "resource": "rep"
        }
      }
    ]
  },
  "resources": [
    {
      "name": "dataPop",
      "path": "data/dataPop.csv",
      "format": "csv",
      "schema": {
        "fields": [
          {
            "name": "date",
            "type": "date"
          },
          {
            "name": "Chaiyaphum",
            "type": "number"
          }
        ]
      }
    },
    {
      "name": "dataEpi",
      "path": "data/dataEpi.csv",
      "format": "csv",
      "schema": {
        "fields": [
          {
            "name": "date",
            "type": "date"
          },
          {
            "name": "Chaiyaphum",
            "type": "number"
          }
        ]
      }
    },
    {
      "name": "dataClim",
      "path": "data/dataClim_Chaiyaphum.csv",
      "format": "csv",
      "schema": {
        "fields": [
          {
            "name": "date",
            "type": "date"
          },
          {
            "name": "maxtemp",
